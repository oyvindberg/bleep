{
  "projects" : {
    "bleep" : {
      "dependencies" : [
        "com.lihaoyi::pprint:0.6.6",
        "com.monovore::decline:2.2.0",
        "org.scalameta:svm-subs:101.0.0"
      ],
      "dependsOn" : [
        "bleep-core",
        "bloop-rifle"
      ],
      "extends" : "template-common-main",
      "platform" : {
        "name": "jvm",
        "mainClass": "bleep.Main"
      }
    },
    "bleep-core" : {
      "dependencies" : [
        "com.lihaoyi::fansi:0.2.14",
        "io.circe::circe-generic:0.14.1",
        "io.circe::circe-parser:0.14.1",
        "io.get-coursier::coursier:2.0.16",
        "net.harawata:appdirs:1.2.1",
        "org.scala-lang:scala-reflect:2.13.7"
      ],
      "extends" : "template-common-main"
    },
    "bleep-tasks" : {
      "dependencies" : "se.sawano.java:alphanumeric-comparator:1.4.1",
      "dependsOn" : "bleep-core",
      "extends" : "template-common-main",
      "resources" : "./liberated/sbt-native-image/plugin/src/main/resources",
      "sources" : [
        "./liberated/bloop-packager/src/main/scala",
        "./liberated/sbt-git-versioning/src/main/scala",
        "./liberated/sbt-native-image/plugin/src/main/scala"
      ]
    },
    "bleep-test" : {
      "dependencies" : "org.scalatest::scalatest:3.2.10",
      "dependsOn" : "bleep",
      "extends" : "template-common",
      "folder" : "./bleep",
      "sbt-scope" : "test",
      "testFrameworks" : [
        "com.novocode.junit.JUnitFramework",
        "munit.Framework",
        "org.scalacheck.ScalaCheckFramework",
        "org.scalatest.tools.Framework",
        "org.scalatest.tools.ScalaTestFramework",
        "org.specs.runner.SpecsFramework",
        "org.specs2.runner.Specs2Framework",
        "org.specs2.runner.SpecsFramework"
      ]
    },
    "bloop-rifle" : {
      "dependencies" : [
        "ch.epfl.scala:bsp4j:2.0.0",
        "com.github.alexarchambault.tmp.ipcsocket:ipcsocket:1.4.1-aa-2",
        "com.google.code.findbugs:jsr305:3.0.2",
        "com.google.code.gson:gson:2.8.9",
        "com.google.errorprone:error_prone_annotations:2.2.0",
        "com.google.guava:guava:27.1-jre",
        "com.google.j2objc:j2objc-annotations:1.1",
        "me.vican.jorge::snailgun-core:0.4.0",
        "org.checkerframework:checker-qual:2.5.2",
        "org.codehaus.mojo:animal-sniffer-annotations:1.17",
        "org.graalvm.nativeimage:svm:20.2.0",
        "org.scala-lang:scala-library:2.13.7"
      ],
      "extends" : "template-common-main"
    },
    "infrastructure" : {
      "dependsOn" : [
        "bleep",
        "bleep-tasks"
      ],
      "extends" : "template-common-main"
    }
  },
  "scripts": {
    "native-image": "infrastructure/bleep.infra.GenNativeImage",
    "jars": "infrastructure/bleep.infra.PackageAll"
  },
  "templates" : {
    "template-common" : {
      "dependencies" : "ch.epfl.scala::bloop-config:1.4.11",
      "scala" : {
        "options" : [
          "-Vimplicits",
          "-Vtype-diffs",
          "-Wdead-code",
          "-Wextra-implicit",
          "-Wnumeric-widen",
          "-Wunused:explicits",
          "-Wunused:implicits",
          "-Wunused:imports",
          "-Wunused:locals",
          "-Wunused:nowarn",
          "-Wunused:params",
          "-Wunused:patvars",
          "-Wunused:privates",
          "-Wvalue-discard",
          "-Xcheckinit",
          "-Xlint:-byname-implicit",
          "-Xlint:adapted-args",
          "-Xlint:constant",
          "-Xlint:delayedinit-select",
          "-Xlint:deprecation",
          "-Xlint:doc-detached",
          "-Xlint:inaccessible",
          "-Xlint:infer-any",
          "-Xlint:missing-interpolator",
          "-Xlint:nullary-unit",
          "-Xlint:option-implicit",
          "-Xlint:package-object-classes",
          "-Xlint:poly-implicit-overload",
          "-Xlint:private-shadow",
          "-Xlint:stars-align",
          "-Xlint:strict-unsealed-patmat",
          "-Xlint:type-parameter-shadow",
          "-encoding",
          "utf8",
          "-explaintypes",
          "-feature",
          "-language:existentials",
          "-language:experimental.macros",
          "-language:higherKinds",
          "-language:implicitConversions",
          "-unchecked"
        ],
        "version" : "2.13.7"
      }
    },
    "template-common-main" : {
      "extends" : "template-common",
      "sbt-scope" : "main"
    }
  },
  "version" : "1"
}