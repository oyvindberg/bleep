{
  "version": "1",
  "templates": {
    "default": {
      "scala": {
        "version": "2.12.6"
      },
      "sbt-scope": "main"
    }
  },
  "projects": {
    "common": {
      "extends": "default",
      "dependencies": [
        "io.circe::circe-generic:0.12.3"
      ]
    },
    "bigquery": {
      "extends": "default",
      "folder": "clients/bigquery",
      "dependsOn": [
        "common"
      ]
    },
    "bigquery-test": {
      "extends": "default",
      "folder": "clients/bigquery",
      "sources": "src/test/scala",
      "dependsOn": [
        "common"
      ]
    }
  }
}